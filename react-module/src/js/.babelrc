{
	"presets": [[
		"@babel/preset-env", {
			"useBuiltIns": "entry",
			"corejs": "3.0.0",
			"targets": {
				"esmodules": true,
				"ie": "11"
			}
		}],
		"@babel/preset-react"
	],
	"plugins": [
		"react-hot-loader/babel",
		"@babel/plugin-proposal-class-properties",
		["@babel/plugin-transform-runtime",
			{
				"regenerator": true
			}
		]
	]
}
